{
  "name": "blehology",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "scripts"
  ],
  "scripts": {
    "sync-env": "node scripts/sync-env.js",
    "dev:backend": "npm --workspace apps/backend run dev",
    "dev:frontend": "npm --workspace apps/frontend run dev",
    "dev:ia": "npm --workspace apps/IA run dev",
    "build": "npm -w apps/backend run build && npm -w apps/frontend run build",
    "start": "npm -w apps/backend run start"
  },
  "engines": {
    "node": ">=18"
  }
}
